           q=0;cleardevice();
        	basic();
         }
          else
         {
          settextstyle(2,0,4);
          outtextxy(50,70,"Already taken");
          goto a15;
		}
		}
		else 
		{
			cleardevice();
			settextstyle(2,0,5);outtextxy(x/2,y/2,"Wrong answer"); 
			delay(2000);
			  
			sprintf(enter,"You won %s rupees",  winning());
			outtextxy(x/2,y/2+50,enter);
			delay(3000);cleardevice();page1();
			
		}
	     break;
	     
	     }case 16:{
	     outtextxy(70,310,"Which Blood Group is known as the Universal Recipient?");
	     outtextxy(80,365,"A: A");
	     outtextxy(405,365,"B: AB");
	     
	     
	     lifeline(); timer(t);
           a16:
	     scanf("%s",comp);
if(strcmp(comp,"B")==0||strcmp(comp,"b")==0)
	     { 
	      cleardevice();
	      settextstyle(2,0,8);
	      setcolor(YELLOW);
	       outtextxy(x/2,y/2,"!!CORRECT!!");

          delay(1000);
          cleardevice();
          
		 }
		 else if(strcmp(comp,"E")==0||strcmp(comp,"e")==0)
		 {settextstyle(2,0,4);outtextxy(50,50,"Not Allowed");goto a16;
		 }
        else if(strcmp(comp,"G")==0||strcmp(comp,"g")==0)
        {  
         if(c==1)
         {
        	a++;
        	c--;
           q=0;cleardevice();
        	basic();
         }
          else
         {
          settextstyle(2,0,4);
          outtextxy(50,70,"Already taken");
          goto a16;
		}
        }
		else 
		{
			cleardevice();
			settextstyle(2,0,5);outtextxy(x/2,y/2,"Wrong answer"); 
			delay(2000);
			  
			sprintf(enter,"You won %s rupees",  winning());
			outtextxy(x/2,y/2+50,enter);
			delay(3000);cleardevice();page1();
			
		}
	     break;
	     
	     }

}
  }
else
{ 

settextstyle(2,0,4);
  outtextxy(100,100,"Already used");

}
}
void main()
{
  int gd=DETECT,gm;
  char data[1000];
  initgraph(&gd,&gm,"C:\\TC\\BGI");
  page1();
  getch();
  }


