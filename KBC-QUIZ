		     if(strcmp(comp,"C")==0||strcmp(comp,"c")==0)
	     {
	      cleardevice();
	      settextstyle(2,0,8);
	      setcolor(YELLOW);
	       outtextxy(x/2,y/2,"!!CORRECT!!");
	       sum++;
	  a++;
	  delay(1000);
	  cleardevice();
	  basic();
		 }
