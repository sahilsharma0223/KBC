  outtextxy(x/2-200,y/2,"You Are a Champion");
  delay(1000);
  outtextxy(x/2-200,y/2 +50,"Come back soon");
  delay(2000);
page1();
 
  }
  }
void lifeline()
{
setcolor(BLACK);
settextstyle(2,0,6);
  if(b==1&&c==1)
   {
      outtextxy(254,266,"50:50");
      outtextxy(376,266,"<<>>");
   }
  else if(b==1&&c!=1)
  {
      outtextxy(254,266,"50:50");
      }

      else if(b!=1 &&c==1)
      {
      outtextxy(376,266,"<<>>");

      }

  }
 char* winning()
 {
	if(sum==0)
	return "0";
	else if(sum==1)
	return "5000";
	else if(sum==2)
	return "10000";
    else if(sum==3)
	return "20000";
	else if(sum==4)
	return "40000";
	else if(sum==5)
	return "80000";
	else if(sum==6)
	return "160000";
	else if(sum==7)
	return "320000";
	else if(sum==8)
	return "640000";
	else if(sum==9)
	return "1250000";
	else if(sum==10)
	return "2500000";
	else if(sum==11)
	return "5000000";
	else if(sum==12)
	return "1 Crore";
	else if(sum==13)
	return "3 Crore";
	else if(sum==14)
	return "5 Crore";
	else if(sum==15)
	return "7 Crore";
 }
void fifty()
{
