		 {
			if(b==1)
              {
			cleardevice();
			q=1;
			basic();
			sum++;
			a++;b--;
			q=0;
			basic();
		 }
            else
            {
              settextstyle(2,0,4);outtextxy(100,100,"Already taken");timer(t);
              goto g3;
}

		 }
	else if(strcmp(comp,"G")==0||strcmp(comp,"g")==0)
	{
